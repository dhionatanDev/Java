package entites;

public class alunos {
	public String aluno;
	public String status;
	public double media;
	public double n1;
	public double n2;
	public double n3;
	
	
	public void media() {
		
		if((n1+n2+n3)<60) {
			media = 60 - (n1+n2+n3);
			status ="Missing";
		}
		else {
			media = n1 + n2+ n3;
			status = "Pass";
		}	
		}
	public String toString() {
		return "Final grade = " + media + "%n" + status;
	}

}
