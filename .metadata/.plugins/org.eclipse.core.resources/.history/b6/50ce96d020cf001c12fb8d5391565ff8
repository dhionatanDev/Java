		Scanner sc = new Scanner(System.in);
		
		int n = 1;
		int r = 0;
		float calc=0;
		int i = 0;
		
		
		
		
		while ( i < n) {
			i = 1;
			
			System.out.println("Informe um numero entre 1 e 60: ");
			n = sc.nextInt();
			
			calc = i%3;
			if(calc == 0) {
				System.out.println(i);
				i = i+1;
			}
		
	}
	
	
	
	}
