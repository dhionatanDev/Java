package entites;

public class Employee {
	public String name;
	public double grossSalary;
	public double tax;
		
	
	public double netSalary() {
		return grossSalary - tax;
	}
	public double incleaseSalary (double incleaseSalary ) {
		return grossSalary += (grossSalary * incleaseSalary/100) ;
	}
	
		}
 
	

