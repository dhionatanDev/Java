package aulaAlgoritimoLista4Atv13;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int qnt, g, i;
		qnt =0;
		g = 1;
		
		Scanner lerTeclado = new Scanner(System.in);
		
		for (i = 0; i <64; i++) {
			qnt += g;
			g = g *2;
		}
	}

}
