package entites;

public class Employee {
	public String name;
	public double grossSalary;
	public double tax;
	
	
	public double netSalary() {
		return grossSalary - tax;
	}
	public void incleaseSalary (double incleaseSalary ) {
		this.grossSalary += this.grossSalary * incleaseSalary/100;
	}
	
	
}
